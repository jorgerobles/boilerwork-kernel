{
  "name": "medaqueno/boilerwork-kernel",
  "description": "Boilerwork Kernel",
  "type": "project",
  "license": "MIT",
  "require": {
    "php": "^8.1.0",
    "beberlei/assert": "^3.3",
    "guzzlehttp/guzzle": "^7.0",
    "illuminate/container": "^9.4",
    "laminas/laminas-diactoros": "^2.8",
    "nesbot/carbon": "^2.57",
    "nikic/fast-route": "^1.3",
    "php-amqplib/php-amqplib": "^3.2",
    "symfony/polyfill-uuid": "^1.25",
    "vlucas/phpdotenv": "^5.4"
  },
  "require-dev": {
    "ext-decimal": "^1.1",
    "deminy/counit": "^0.2.0",
    "openswoole/ide-helper": "~4.9",
    "php-decimal/php-decimal": "^1.1",
    "php-decimal/stubs": "^1.1",
    "phpunit/phpunit": "^9.5",
    "ukko/phpredis-phpdoc": "5.0.1-beta8"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Boilerwork\\": "src/Boilerwork/"
    },
    "files": [
      "src/Helpers/functions.php"
    ]
  },
  "authors": [
    {
      "name": "CarlosJ - medaqueno"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true
}
